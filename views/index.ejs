<!-- index.ejs -->

<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('parts/head') %>
</head>
<body class="container">

<header>
    <%- include('parts/header') %>
</header>

<main>
<div class="row">
	<div class="col-xs-3">
		<%- include('parts/quicklinks') %>
	</div>
	
	<div class="col-xs-9">
		<div class="jumbotron">
			<ul>
				<% data.forEach(function(category) { %>
					<li><a href="category/<%= category.CAT %>"><%= category.CAT %></a></li>
				<% }); %>
			</ul>
		</div>
	</div>
</div>
</main>

</body>
</html>