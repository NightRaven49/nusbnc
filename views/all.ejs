<!-- all.ejs -->

<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('parts/head') %>
</head>
<body class="container">

<header>
    <%- include('parts/header') %>
</header>

<main>
<div class="row">
	<div class="col-xs-2">
		<%- include('parts/quicklinks') %>
	</div>
	
	<div class="col-xs-10">
		<div class="jumbotron">
			<h2>Bots</h2>
			<ul>
				<% bots.forEach(function(bot) { %>
					<li><%= bot.NAME %></li>
				<% }); %>
			</ul>
			<h2>Announcement Channels</h2>
			<ul>
				<% channels.forEach(function(chan) { %>
					<li><%= chan.NAME %></li>
				<% }); %>
			</ul>
		</div>
    </div>
</main>

</body>
</html>